
function mostFrequentNumber(arr) {
  if (!Array.isArray(arr)) {
    throw new Error("Input must be an array.");
  }
  const count = {};
  let maxNum = null;
  let maxCount = 0;
  for (let num of arr) {
    count[num] = (count[num] || 0) + 1;
    if (count[num] > maxCount) {
      maxCount = count[num];
      maxNum = num;
    }
  }
  return maxNum;
}

const input = [1, 3, 2, 3, 4, 3, 5];
console.log("Most frequent number:", mostFrequentNumber(input));